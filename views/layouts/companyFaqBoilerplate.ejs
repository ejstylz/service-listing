    <script type="text/javascript">
        $(document).ready(function(){
          $(".set > a").on("click", function(){
            if($(this).hasClass('active')){
              $(this).removeClass("active");
              $(this).siblings('.existing_faq').slideUp(200);
              $(".set > a i").removeClass("fa-minus").addClass("fa-plus");
            }else{
              $(".set > a i").removeClass("fa-minus").addClass("fa-plus");
            $(this).find("i").removeClass("fa-plus").addClass("fa-minus");
            $(".set > a").removeClass("active");
            $(this).addClass("active");
            $('.existing_faq').slideUp(200);
            $(this).siblings('.existing_faq').slideDown(200);
            }

          });
        });

        $('.set > a').click(function(e){
            e.preventDefault();
        });
        
        $(document).ready(function () {
            $('.add_faq_button').on('click', function () {
                $('.add_faq_input_field').toggleClass('active');
                $('.add_faq_button').toggleClass('active');
            });
            $('.cancel_faq').on('click', function () {
                $('.add_faq_input_field').removeClass('active');
                $('.add_faq_button').removeClass('active');
            });
        });
        
        $(document).ready(function () {
            $('.add_faq_button2').on('click', function () {
                $('.add_faq_input_field2').toggleClass('active');
                $('.add_faq_button2').toggleClass('active');
            });
            $('.cancel_faq2').on('click', function () {
                $('.add_faq_input_field2').removeClass('active');
                $('.add_faq_button2').removeClass('active');
            });
        });
        
        
          $(window).on("scroll", function() {
                if($(window).scrollTop() > 400) {
                      $('.update_now_button_section').addClass('active');
                }

                else {
                      $('.update_now_button_section').removeClass('active');
                }
          })
    </script>
    
