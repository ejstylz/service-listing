						</div>
						<div class="col-md-6">
							<div class="cp_about_slider">
								<!--Carousel Wrapper-->
								<div id="carousel-thumb" class="carousel slide carousel-fade carousel-thumbnails"
									data-ride="carousel">

									<!--Slides-->
									<div class="carousel-inner" role="listbox">
										<div class="carousel-item active">
											<% if (mediaPhoto[0]) { %>
											<img src="<%= mediaPhoto[0].imageUrl %>" class="img-fluid" alt="" />
											<% }; %>

										</div>

										<% for (var i = 1; i < mediaPhoto.length; i++) { %>
										<div class="carousel-item">
											<img src="<%= mediaPhoto[i].imageUrl %>" class="img-fluid" alt="" />
										</div>
										<% }; %>

									</div>
									<!--/.Slides-->

									<% if (mediaPhoto[0]) { %>
									<ol class="carousel-indicators">
										<li data-target="#carousel-thumb" data-slide-to="0" class="active">
											<img src="<%= mediaPhoto[0].imageUrl %>" class="d-block w-100 img-fluid"
												alt="" />
										</li>
										<% for (var i = 1; i < mediaPhoto.length; i++) { %>
										<li data-target="#carousel-thumb" data-slide-to="<%= i %>">
											<img src="<%= mediaPhoto[i].imageUrl %>" class="d-block w-100 img-fluid"
												alt="" />
										</li>
										<% } %>
									</ol>
									<% } %>


									<!--Controls-->
									<a class="carousel-control-prev" href="#carousel-thumb" role="button"
										data-slide="prev">
										<span class="carousel-control-prev-icon" aria-hidden="true"></span>
										<span class="sr-only">Previous</span>
									</a>
									<a class="carousel-control-next" href="#carousel-thumb" role="button"
										data-slide="next">
										<span class="carousel-control-next-icon" aria-hidden="true"></span>
										<span class="sr-only">Next</span>
									</a>
									<!--/.Controls-->
								</div>
								<!--/.Carousel Wrapper-->
							</div>
						</div>
					</div>
